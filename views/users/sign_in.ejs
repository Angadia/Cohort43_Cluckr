<%- include("../partials/header") %>
  <main class="main">
    <article class="sign_in">
      <div class="sign_in_form">
        <form action="/users/sign_in" method="POST">
          <label class="sign_in_form_input_label">Username</label>
          <input type="text" name="username" placeholder="e.g. Madam Welminster" class="sign_in_form_input_text">
          <div class="sign_in_form_submit">
            <input type="submit" value="Sign In" class="sign_in_form_submit_btn">
          </div>
        </form>
      </div>
    </article>
  </main>
<%- include("../partials/footer") %>
